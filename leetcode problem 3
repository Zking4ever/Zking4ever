class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        char chars[];
        int counter;
        for(int i=0;i<s.length();i++){
            if(!checksAlreadyExistance(s[i],chars)){
                    counter++;
            }
        }
        return counter;
    }

    bool checksAlreadyExistance(char c,char array[]){
            for(int i=0;i<array.length();i++){
                if(c==array[i]){
                    return true;
                }
            }
            return false;
    }
};